{
  "name": "nuxt-typescript-template",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "nokl"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build --modern=client",
    "build:dev": "cross-env NODE_ENV=development nuxt-ts build --modern=client",
    "build:stg": "cross-env NODE_ENV=staging nuxt-ts build --modern=client",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "start:dev": "cross-env NODE_ENV=development nuxt-ts start",
    "start:stg": "cross-env NODE_ENV=staging nuxt-ts start",
    "generate": "cross-env NODE_ENV=production nuxt-ts generate --modern=client",
    "generate:dev": "cross-env NODE_ENV=development nuxt-ts generate --modern=client",
    "generate:stg": "cross-env NODE_ENV=staging nuxt-ts generate --modern=client",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "yarn lint:eslint -- --fix",
    "fix:prettier": "yarn lint:prettier -- --write"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.1",
    "@nuxtjs/pwa": "3.3.5",
    "nuxt": "2.15.3"
  },
  "devDependencies": {
    "@nuxt/types": "2.15.3",
    "@nuxt/typescript-build": "2.0.4",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config": "5.0.0",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/style-resources": "1.0.0",
    "@types/fibers": "3.1.0",
    "@types/sass": "1.16.0",
    "babel-eslint": "10.1.0",
    "core-js": "3.9.0",
    "cross-env": "7.0.3",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-nuxt": "2.0.0",
    "fibers": "5.0.0",
    "npm-run-all": "4.1.5",
    "postcss-css-variables": "0.17.0",
    "prettier": "2.2.1",
    "ress": "3.0.0",
    "sass": "1.32.8",
    "sass-loader": "10.1.1"
  }
}
